    <script>
        // Prevenir comportamientos del navegador
        if (window.navigator.standalone || window.matchMedia('(display-mode: standalone)').matches) {
            document.addEventListener('touchmove', (e) => {
                if (e.scale !== 1) {
                    e.preventDefault();
                }
            }, { passive: false });
            
            document.addEventListener('gesturestart', (e) => {
                e.preventDefault();
            });
        }
    </script>
