{
  "name": "InnoPro",
  "short_name": "InnoPro",
  "description": "Sistema de gesti√≥n Damabrava",
  "start_url": "/",
  "display": "fullscreen",
  "background_color": "#ffffff",
  "theme_color": "#1a1a1a",
  "form_factor": "wide",
  "id": "InnoProho",
  "orientation": "any",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "screenshots": [
    {
      "src": "/screenshots/screenshot1.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Pantalla principal de InnoPro"
    }
  ],
  "icons": [
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "sync": {
    "periodic": true,
    "minPeriod": 60,
    "maxPeriod": 300
  },
  "background_sync": {
    "sync_interval": "1d",
    "retry_delay": "1h",
    "max_retries": 3
  },
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://app-produccion-db.vercel.app/manifest.json"
    }
  ],
  "prefer_related_applications": false,
  "shortcuts": [],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "media",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+innopro",
      "url": "/%s"
    }
  ],
  "display_override": ["fullscreen"],
  "categories": ["productivity", "business"],
  "dir": "ltr",
  "iarc_rating_id": "",
  "scope": "/"
}